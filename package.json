{
  "name": "llm-service-provider",
  "version": "2.23.0",
  "description": "LLM service provider with API key management and streaming capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "README_EN.md",
    "index.ts"
  ],
  "scripts": {
    "build": "tsc && cp src/index.css dist/",
    "prepare": "npm run build",
    "publish:patch": "./publish.sh patch",
    "publish:minor": "./publish.sh minor",
    "publish:major": "./publish.sh major",
    "publish:dry": "npm publish --dry-run",
    "publish:local": "npm run build && npm publish",
    "screenshot": "node screenshot.js",
    "screenshot:simple": "node screenshot_simple.js"
  },
  "keywords": [
    "llm",
    "api",
    "service",
    "provider",
    "deepseek",
    "gemini",
