    "mindmap",
    "visualization"
  ],
  "author": "qcgm1978",
  "repository": {
    "type": "git",
    "url": "https://github.com/qcgm1978/llm-service-provider.git"
  },
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.7.0",
    "query-string": "^9.3.1",
    "reactflow": "^11.11.4",
    "sse.js": "^2.7.1",
    "uuid": "^13.0.0"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.10",
